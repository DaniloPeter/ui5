{
  "_version": "1.58.0",
  "sap.app": {
    "id": "ui5.testapp",
    "type": "application",
    "title": "Test Application",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "type": "View",
        "viewType": "XML",
        "path": "ui5.testapp.view",
        "controlId": "app",
        "controlAggregation": "pages"
      },
      "routes": [
        {
          "pattern": "",
          "name": "overview",
          "target": "overview"
        },
        {
          "pattern": "detail",
          "name": "detail",
          "target": "detail"
        }
      ],
      "targets": {
        "overview": {
          "id": "overview",
          "name": "Overview"
        },
        "detail": {
          "id": "detail",
          "name": "Detail"
        }
      }
    },
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {}
      }
    },
    "contentDensities": { "cozy": true, "compact": false },
    "models": {
      "task": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "TaskData.json"
      }
    },
    "rootView": {
      "viewName": "ui5.testapp.view.App",
      "type": "XML",
      "id": "app"
    },
    "resources": {
      "css": [{ "uri": "css/style.css" }]
    }
  }
}
